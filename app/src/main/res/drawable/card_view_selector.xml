<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <inset android:insetLeft="3dp" android:insetTop="4dp" android:insetRight="3dp" android:insetBottom="4dp">
            <shape>
                <stroke android:width="2dp" android:color="@color/lineColor" />
                <corners android:radius="3dp" />
            </shape>
        </inset>
    </item>
</selector>