<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="PotentialRingView" parent="">
        <item name="bgColor">@color/dark_gray</item>
        <item name="fgColorStart">@color/orange_700</item>
        <item name="fgColorEnd">@color/orange_200</item>
        <item name="strokeWidth">@dimen/strokeWidth</item>
        <item name="android:layout_width">@dimen/potentialProgressViewSize</item>
        <item name="android:layout_height">@dimen/potentialProgressViewSize</item>
    </style>

    <style name="PercentageView" parent="ParentPercentageView">
        <item name="progressBackgroundColor">@color/dark_gray</item>
        <item name="progressColor">@color/orange_200</item>
    </style>

    <!--Dashboard-->
    <style name="PotentialProgress" parent="PotentialProgressParent">
        <item name="rcProgressColor">@color/potential_foreground</item>
        <item name="rcSecondaryProgress">@color/potential_background</item>
    </style>
</resources>